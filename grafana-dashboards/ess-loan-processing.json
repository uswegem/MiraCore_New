{
  "dashboard": {
    "id": null,
    "uid": "ess-loan-processing",
    "title": "ESS Loan Processing Monitor",
    "description": "Detailed monitoring of loan processing workflows, UTUMISHI integration, and MIFOS operations",
    "tags": ["ess", "loans", "utumishi", "mifos", "processing"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Loan Application Flow",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate({job=\"ess-app\"} |~ \"TOP_UP_OFFER_REQUEST|LOAN_OFFER_REQUEST\" [5m])) by ()",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            },
            "legendFormat": "Loan Requests"
          },
          {
            "expr": "sum(rate({job=\"ess-app\"} |~ \"LOAN_OFFER_RESPONSE|TOP_UP_OFFER_RESPONSE\" [5m])) by ()",
            "refId": "B",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            },
            "legendFormat": "Loan Responses"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "UTUMISHI Integration Status",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate({job=\"ess-app\"} |~ \"UTUMISHI.*success\" [5m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "MIFOS Integration Status",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate({job=\"ess-app\"} |~ \"MIFOS.*success\" [5m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 4,
        "title": "Loan Processing Errors",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate({job=\"ess-error\"} |~ \"loan|mapping|UTUMISHI|MIFOS\" [5m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 4}
      },
      {
        "id": 5,
        "title": "Transaction Safety Events",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate({job=\"ess-app\"} |~ \"DBTransaction|retry|rollback\" [5m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 4}
      },
      {
        "id": 6,
        "title": "Loan Processing Timeline",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"ess-app\"} |~ \"ESS[0-9]+.*TOP_UP_OFFER_REQUEST|LOAN_OFFER_REQUEST|createInitialMapping|createOrUpdateWithClientData\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 12, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 7,
        "title": "Message Type Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (message_type) (count_over_time({job=\"ess-app\"} |~ \"TOP_UP_OFFER_REQUEST|LOAN_OFFER_REQUEST|DISBURSEMENT_NOTIFICATION|ACK\" [1h]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 8,
        "title": "Health Monitor Events",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"ess-app\"} |~ \"Health Monitor|healthCheck|monitor\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": false,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "links": []
  }
}