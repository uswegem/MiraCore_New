{
  "dashboard": {
    "id": null,
    "uid": "ess-error-analysis",
    "title": "ESS Error Analysis & Debugging",
    "description": "Comprehensive error analysis, debugging tools, and system health monitoring",
    "tags": ["ess", "errors", "debugging", "analysis"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Critical Errors (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(count_over_time({job=\"ess-error\"} |~ \"Error|Exception|Failed\" [1h]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Database Transaction Failures",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(count_over_time({job=\"ess-error\"} |~ \"DBTransaction.*failed|transaction.*error|rollback\" [1h]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Loan Mapping Failures",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(count_over_time({job=\"ess-error\"} |~ \"loan.*mapping.*failed|createInitialMapping.*error|createOrUpdateWithClientData.*error\" [1h]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Integration Failures",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(count_over_time({job=\"ess-error\"} |~ \"UTUMISHI.*error|MIFOS.*error|connection.*failed\" [1h]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Error Trends Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate({job=\"ess-error\"} |~ \"Error|Exception\" [5m])) by ()",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            },
            "legendFormat": "All Errors"
          },
          {
            "expr": "sum(rate({job=\"ess-error\"} |~ \"loan.*mapping\" [5m])) by ()",
            "refId": "B",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            },
            "legendFormat": "Loan Mapping Errors"
          },
          {
            "expr": "sum(rate({job=\"ess-error\"} |~ \"DBTransaction\" [5m])) by ()",
            "refId": "C",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            },
            "legendFormat": "Database Errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "Recent Error Details",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"ess-error\"} |= \"\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 12, "w": 24, "x": 0, "y": 12}
      },
      {
        "id": 7,
        "title": "Error Categories",
        "type": "table",
        "targets": [
          {
            "expr": "topk(15, sum by (error_type) (count_over_time({job=\"ess-error\"} [2h])))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "System Recovery Events",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"ess-app\"} |~ \"retry|recovered|fallback|circuit.*breaker\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "ef69p8jegs4xsf"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-3h",
      "to": "now"
    },
    "refresh": "1m",
    "schemaVersion": 37,
    "version": 1,
    "links": []
  }
}